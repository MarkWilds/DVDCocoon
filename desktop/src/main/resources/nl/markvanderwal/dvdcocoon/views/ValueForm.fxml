<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox spacing="8.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="valueLabel" text="IdValueType" />
                  <ListView fx:id="dataListView" VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="4.0" top="8.0" />
               </padding>
            </VBox>
            <AnchorPane HBox.hgrow="ALWAYS">
               <children>
                  <VBox layoutX="8.0" layoutY="8.0" spacing="8.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Label text="Acties:" />
                        <Button fx:id="saveButton" mnemonicParsing="false" prefWidth="128.0" text="Bewaar" />
                        <Button fx:id="editButton" mnemonicParsing="false" prefWidth="128.0" text="Bewerk" />
                        <Button fx:id="deleteButton" mnemonicParsing="false" prefWidth="128.0" text="Verwijder" />
                     </children>
                  </VBox>
                  <VBox layoutX="16.0" layoutY="439.0" prefWidth="117.0" spacing="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                     <children>
                        <Label text="Naam:" />
                        <TextField fx:id="nameTextField" prefWidth="128.0" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="8.0" left="4.0" right="8.0" top="8.0" />
               </padding>
            </AnchorPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
